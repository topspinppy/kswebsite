{
  "parser": "babel-eslint",
  "extends": ["prettier", "prettier/standard", "prettier/react"],
  "plugins": ["flowtype", "standard", "promise"],
  "rules": {
    "jsx-quotes": ["error", "prefer-double"],
    "space-before-function-paren": "off",
    "comma-dangle": "off",
    "indent": 0,
    "no-undef": "off",
    "react/prop-types": "off",
    "no-unused-vars": "off"
  }
}
